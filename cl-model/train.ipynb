    "def scale(x):\n",
    "    if \"kg\" in x:\n",
    "        return float(x[:-2])*1000\n",
    "    elif \"g\" in x:\n",
    "        return float(x[:-1])\n",
    "    elif \"ml\" in x:\n",
    "        return float(x[:-2])\n",
    "    elif \"l\" in x or \"L\" in x:\n",
    "        return float(x[:-1])*1000\n",
    "    else:\n",
    "        return float(x)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "metadata": {},
   "outputs": [],
   "source": [
    "df['weight'] = df['weight'].apply(scale)\n",
    "chosen_features = [\"expire_in\", \"remaining_quantity\", \"price\", \"weight\"]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
